<style lang="less">
// @font-face {
//     font-family: 'iconfont';
//     src: url(data:application/font-woff2;charset=utf-8;base64,) format('truetype');
//     font-weight: normal;
//     font-style: normal;
// }
@font-face {
  font-family: 'iconfont';
  src: url(data:font/truetype;charset=utf-8;base64,) format('truetype');
  font-weight: normal;
  font-style: normal;
}

.information{
  &.information-show{
	  display: block;
  }
  .information-content{
	  padding: 20px;
  }
  display: none;
  position: fixed;
  z-index: 2;
  bottom: 0;
  width: 100%;
	margin: 0 auto;
  height: 300rpx;
	opacity: 1;
	overflow: hidden;
	background-color: #fff;
	font-size: 14px;
	line-height: 18px;
  word-break: break-all;
  will-change: transform;
	/* transform: translateX(-50%) translateY(-50%); */
	animation: slideInUp .3s ;
  .word{
    font-size: 16pt;
    font-weight: 400;
    color:rgb(32, 158, 133);
  }
  .pron{
    color:gray;
    margin-left: 10rpx;
  }
  .audio{
    font-family: 'iconfont';
    float:right;
    margin-top:22rpx;
    color:rgb(32, 158, 133);
    &:before{
      content: '\e605';
      margin-right: 10rpx;
      font-size: 16pt;
    }
  }
  .definition{
    margin-top: 16rpx;
    .definition-item{
      display: block;
    }
  }
}
.slidedown{
  animation: slideOutDown .3s;
}
.slidebar{
  width: 100%;
  background-color: #eee;
  height: 60rpx;
  color:#bdbdbd;
  text-align: center;
  line-height: 60rpx;
  font-family: 'iconfont';
  &:before{
      content: '\e6e0';
      margin-right: 10rpx;
      font-size: 16pt;
    }
}
@keyframes slideInUp {
  from {
    transform: translate3d(0, 100%, 0);
    visibility: visible;
  }

  to {
    transform: translate3d(0, 0, 0);
  }
}
@keyframes slideOutDown {
  from {
    transform: translate3d(0, 0, 0);
  }

  to {
    visibility: hidden;
    transform: translate3d(0, 100%, 0);
  }
}

</style>

<template>
		<view class="information" style="{{cssText}}" @tap.stop="stopPropagation"  :class="{information-show:!!visible,slidedown:isSlidingDown}">
      <view class="slidebar" @touchmove="touchmove" @touchstart.stop="touchstart"></view>
      <view class="information-content">
        <text class="word">{{word}}</text>
        <text class="pron">{{pron}}</text>
        <audio class="audio" id="audio" src="{{audio}}" @tap="play"/>
        <view class="definition">
          <repeat for="{{definition}}">
            <text class="definition-item">{{item}}</text>
          </repeat>
        </view>
      </view>
    </view>
</template>

<script>
import wepy from 'wepy'

export default class TipBox extends wepy.component {
  data = {
    visible: false,
    position: '',
    margin: '',
    word: '',
    audio: '',
    pron: '',
    definition: '',
    isSlidingDown: false,
    isDetail: false
  };

  computed = {
    cssText() {
      let styles = {}
      if (this.position === 'top') {
        styles.position = this.position
        styles.top = this.margin || ''
      } else if (this.position === 'bottom') {
        styles.position = this.position
        styles.top = 'initial'
        styles.bottom = this.margin || ''
      }
      let css = Object.keys(styles)
      .map(key => (key + ':' + styles[key]))
      .join(';')
      return css
    }
  };
  methods={
    stopPropagation(e) {
    },
    play() {
      this.audioCtx.play()
    },
    touchmove(e) {
      let currentY = e.touches[0].pageY
      let isSlideDown = currentY > this.lastY
      this.handleSlide(isSlideDown)
    },
    touchstart(e) {
      this.lastY = e.touches[0].pageY
    }
  }
  show({word, options}) {
    if (!word) {
      return
    }
    this.visible = true
    this.word = word
    this.$apply()
    wx.request({
      url: `https://api.shanbay.com/bdc/search/?word=${word}`,
      success: (res) => {
        if (res.data.msg === 'SUCCESS') {
          var {audio, pron, definition} = res.data.data
          this.audio = audio
          this.pron = pron ? `/${pron}/` : ''
          this.definition = definition.split(/\r\n|\n|\r/)
          this.audioCtx.setSrc(audio)
        } else {
          this.pron = ''
          this.audioCtx.setSrc('')
          this.definition = [].concat(res.data.msg)
        }
        this.$apply()
      }
    })
  }
  hide() {
    this.isSlidingDown = true
    this.$apply()
    setTimeout(() => {
      this.visible = false
      this.word = ''
      this.audio = ''
      this.pron = ''
      this.definition = ''
      this.position = ''
      this.margin = ''
      this.isSlidingDown = false
      this.isDetail = false
      this.$apply()
    }, 300)
  }
  handleSlide(isSlideDown) {
    if (isSlideDown) {
      if (this.isSlidingDown === false) {
        this.hide()
      }
    } else {
      this.isDetail = true
    }
  }
  onLoad() {
    this.audioCtx = wx.createAudioContext('audio')
  }
}
</script>
